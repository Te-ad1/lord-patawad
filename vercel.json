{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/app/view/app.html"
    },
    {
      "src": "^/works$",
      "dest": "/app/view/works.html"
    },
    {
      "src": "^/playground$",
      "dest": "/app/view/playground.html"
    },
    {
      "src": "^/contact$",
      "dest": "/app/view/contact.html"
    },
    {
      "src": "^/resume$",
      "dest": "/app/view/resume.html"
    },
    {
      "src": "/app/resources/(.*)",
      "dest": "/app/resources/$1"
    },
    {
      "src": "/resources/(.*)",
      "dest": "/app/resources/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/app/view/app.html"
    }
  ]
} 
