{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/app/view/works.html"
    },
    {
      "src": "/app/resources/(.*)",
      "dest": "/app/resources/$1"
    },
    {
      "src": "/app/view/(.*)",
      "dest": "/app/view/$1"
    },
    {
      "src": "/resources/(.*)",
      "dest": "/app/resources/$1"
    },
    {
      "src": "/view/(.*)",
      "dest": "/app/view/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/app/view/works.html"
    }
  ]
} 
