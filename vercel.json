{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "app"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/view/app.html"
    },
    {
      "src": "/resources/(.*)",
      "dest": "/resources/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/view/app.html"
    }
  ]
} 